require(["jquery"],function(r){"use strict";var e=r.parseJSON(localStorage.getItem("mage-cache-storage"));"object"==typeof e&&null!==e&&e.hasOwnProperty("wishlist")&&"object"==typeof e.wishlist&&"object"==typeof e.wishlist.items&&r(document).ready(function(){var t=r(".action.towishlist:not(.already)"),i=[],o={};r.each(e.wishlist.items,function(t,e){var a=parseInt(e.product_id),s=e.delete_item_params;"number"==typeof a&&!isNaN(a)&&0<a&&(i.push(a),"string"==typeof s&&(o[a]=s))}),0<t.length&&t.each(function(){var t=r(this),e=r(this).data("post"),a=void 0!==e.data&&parseInt(e.data.product),s=void 0!==e.data&&e.data.uenc;"number"==typeof a&&!isNaN(a)&&-1<i.indexOf(a)&&(t.addClass("already"),"string"==typeof o[a]&&(o[a]=o[a].replace('"uenc":""','"uenc":"'+s+'"'),t.data("post",o[a])))})})});